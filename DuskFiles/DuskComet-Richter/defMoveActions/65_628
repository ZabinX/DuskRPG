if ! isPlayer trigger
	endscript
end
if hasCondition trigger chicken
	livingthing guard1 local trigger "Pirate Assassin"
		if = guard1 locX 67
			if = guard1 locY 627
			set guard1 locxy 215 191
			end
		end
	livingthing guard2 local trigger "BlackBeard"
		if = guard2 locX 63
			if = guard2 locY 627
			set guard2 locxy 215 191
			end
		end
	set trigger locxy 75 628
	endscript
end
if hasCondition trigger shenville_0
	livingthing guard2 local trigger "Pirate Assassin"
		if = guard2 locX 67
			if = guard2 locY 627
			set guard2 locxy 215 191
			end
		end
	livingthing guard3 local trigger "BlackBeard"
		if = guard3 locX 63 
			if = guard3 locY 627
			set guard3 locxy 215 191
			end
		end
	set trigger locxy 75 628
	endscript
end



