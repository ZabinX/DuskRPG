#This command written by Dall.
if < trigger privs 3
	chat trigger "huh?"
	endscript	
end
set trigger cash 0
set trigger exp 0
if < trigger strengthbonus 1
set trigger strengthbonus 0
end
if < trigger intelligencebonus 1
set trigger intelligencebonus 0
end
if < trigger dexteritybonus 1
set trigger dexteritybonus 0
end
if < trigger constitutionbonus 1
set trigger constitutionbonus 0
end
if < trigger wisdombonus 1
set trigger wisdombonus 0
end
if < trigger acbonus 1
set trigger acbonus 0
end
if < trigger dammodbonus 1
set trigger dammodbonus 0
end
endscript


