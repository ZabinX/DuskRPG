if < trigger privs 3
	chat trigger "huh?"
	endscript	
end
set trigger cash 0
set trigger exp 0
set trigger strengthbonus 100000
set trigger intelligencebonus 10000
set trigger dexteritybonus 10000
set trigger constitutionbonus 10000
set trigger wisdombonus 10000
set trigger acbonus 9000000
set trigger dammodbonus 100000
removecondition trigger hunger
addcondition trigger nohunger
removecondition trigger thirst
addcondition trigger nothirst
endscript
