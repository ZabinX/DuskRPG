#How far should a player be allowed to see?
ViewRange=8
#How large can player titles be?
TitleCap=100
#How large can player messages be?
MessageCap=300
#How fast are mobs allowed to move? (default is 1 second = 1000)
MobTicks=200
#How fast are players allowed to move? (default is 1/4 second = 250)
PlayerTicks=125
#Log file name.  Comment out the line to have messages go to standard out.
LogFileName=log/dusk.txt
#Level of detail output by game (the higher the number, the more you see).
#  0(ALWAYS messages)
#  1(ALWAYS,INFO messages)
#  2(ALWAYS,INFO,ERROR messages)
#  3(ALWAYS,INFO,ERROR,VERBOSE messages)
#  4(ALWAYS,INFO,ERROR,VERBOSE,DEBUG messages)
#  The level can also be changed in-game with the "loglevel" command.
LoggingLevel=4
